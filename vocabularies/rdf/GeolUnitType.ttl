<https://data.geoscience.fr/ncl/GeolUnitType/_7>
        a                  reg:RegisterItem ;
        rdfs:label         "Pedostratigraphic Unit"@en , "Unité pédostratigraphique"@fr ;
        dct:dateAccepted   "2020-04-08T12:25:29.83Z"^^xsd:dateTime ;
        dct:dateSubmitted  "2020-04-08T12:25:14.87Z"^^xsd:dateTime ;
        dct:description    "Unité géologique qui représente un seul horizon pédologique dans une séquence de strates (consolidées ou non). La présence d'une unité géologique sus-jacente est requise, mais localement, l'horizon du sol peut être à la surface de la Terre (auquel cas il peut coïncider avec un pédoderme)."@fr , "Geologic unit that represents a single pedologic horizon in a sequence of strata (consolidated or non-consolidated). The presence of an overlying geologic unit is required, but locally the soil horizon may be at the Earth surface (in which case is may be coincident with a Pedoderm). See discussion at <https://www.seegrid.csiro.au/twiki/bin/view/CGIModel/PedostratigraphicUnit>"@en ;
        dct:modified       "2022-12-07T15:15:29.274Z"^^xsd:dateTime ;
        reg:definition     [ reg:entity       <https://data.geoscience.fr/ncl/GeolUnitType/7> ;
                             reg:sourceGraph  <https://data.geoscience.fr/ncl/GeolUnitType/_7:4#graph>
                           ] ;
        reg:itemClass      skos:Concept ;
        reg:notation       7 ;
        reg:register       <https://data.geoscience.fr/ncl/GeolUnitType> ;
        reg:status         reg:statusStable ;
        reg:submitter      [ foaf:accountName  "h.bressan@brgm.fr" ;
                             foaf:name         "Hélène Bressan"
                           ] ;
        owl:versionInfo    5 .

