        reg:notation       19 ;
        reg:register       <https://data.geoscience.fr/ncl/GeolUnitType> ;
        reg:status         reg:statusStable ;
        reg:submitter      [ foaf:accountName  "h.bressan@brgm.fr" ;
                             foaf:name         "Hélène Bressan"
                           ] ;
        owl:versionInfo    5 .

